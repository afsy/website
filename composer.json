{
    "name": "afsy/site",
    "description": "Site internet de l'AFSY",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "doctrine/doctrine-migrations-bundle": "^1.3|^2.0",
        "doctrine/orm": "^2.5.11",
        "erusev/parsedown": "^1.7",
        "fpn/doctrine-extensions-taggable": "^0.9.0",
        "fpn/tag-bundle": "dev-patch-1",
        "hwi/oauth-bundle": "^1.3.0",
        "knplabs/knp-markdown-bundle": "^1.7",
        "knplabs/knp-paginator-bundle": "^4.0",
        "nesbot/carbon": "^2.27",
        "php-http/guzzle6-adapter": "^2.0.1",
        "php-http/httplug-bundle": "^1.16.0",
        "sensio/framework-extra-bundle": "^5.5",
        "sonata-project/admin-bundle": "^3.54",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "4.4.*",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^2.12",
        "twig/markdown-extra": "^2.12"
    },
    "require-dev": {
        "behat/symfony2-extension": "^2.1",
        "bossa/phpspec2-expect": "^3.1",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Behat\\": "features/bootstrap/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "repositories": [
        {
            "url": "https://github.com/webhdx/FPNTagBundle",
            "type": "vcs"
        }
    ]
}
